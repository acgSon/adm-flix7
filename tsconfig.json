{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2017",
    "module": "commonjs",
    "baseUrl": "./src",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": false
  },
  "include": ["src/"]
}
